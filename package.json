{
  "name": "whiskey-blog-node",
  "version": "0.0.1",
  "description": "I'm going to try to make a specialized blog app with Node now",
  "main": "app.js",
  "scripts": {
    "test": "mocha",
    "sass": "node-sass public/static/sass/ -o public/static/style/ --output-style compressed",
    "sass-watch": "node-sass public/static/sass/ -o public/static/style/ --output-style compressed -w",
    "uglify": "uglifyjs public/static/js/lodash.min.js public/static/js/scripts.js -c -o public/static/js/scripts.min.js",
    "migrate": "migrate up",
    "build": "npm install && npm run migrate && npm run sass && npm run uglify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjgavlick/whiskey-blog-node.git"
  },
  "author": "Tim Gavlick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tjgavlick/whiskey-blog-node/issues"
  },
  "homepage": "https://github.com/tjgavlick/whiskey-blog-node#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.4.0"
  },
  "dependencies": {
    "bcrypt": "1.0.2",
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "migrate": "^0.2.3",
    "node-sass": "^4.5.3",
    "pg-promise": "^5.7.1",
    "twig": "^1.10.5",
    "twig-markdown": "^0.1.2",
    "uglify-js": "^3.0.18"
  }
}
